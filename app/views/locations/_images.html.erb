<div id="imageFeed">
<% if @images.any? %>
<div class="well">
<div class="row" id="imagesList">
  <div class="col-xs-12">
    <% @images.each do |img| %>
    <%= render partial: "images/image", locals: {img: img} %>
    <% end %>
  </div>
</div>
</div>
<% end %>

<%= render "locations/share_photo_button" %>
</div>
